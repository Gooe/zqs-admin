{
	"name" : "zqscjj/zqs-admin",
	"description" : "基于最新tp6和layuiadmin的通用后台管理框架，强大自由的构建器，极少的代码打造完善的后台管理，开发后台轻而易举！",
	"license" : "Apache-2.0",
	"authors" : [{
			"name" : "zqs",
			"email" : "zqscjj@163.com"
		}
	],
	"require" : {
		"php" : ">=7.1",
		"topthink/framework" : "^6.0.0",
		"topthink/think-captcha" : "^3.0.2",
		"topthink/think-multi-app" : "^v1.0.12",
		"liliuwei/thinkphp-jump" : "^1.5"
	},
	"autoload" : {
		"psr-4" : {
			"zqs\\admin\\" : "src/"
		},
		"files" : [
			"src/helper.php"
		]
	},
	"extra" : {
		"think" : {
			"services" : [
				"zqs\\admin\\Service"
			],
			"config" : {
				"admin" : "src/admin.config.php"
			}
		}
	},
	"minimum-stability" : "dev"
}